<?xml version="1.0" encoding="ISO-8859-1"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:kursliste="http://xmlns.estv.admin.ch/ictax/2.2.0/kursliste" targetNamespace="http://xmlns.estv.admin.ch/ictax/2.2.0/kursliste" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.2.0" xml:lang="de-CH">
	<xs:element name="kursliste" type="kursliste:kursliste">
		<xs:annotation>
			<xs:documentation xml:lang="de-CH">Basiselement für das XML Schema zum Datenaustausch zwischen Kursliste und Dritten (V 2.2.0/05.11.2025).</xs:documentation>
			<xs:documentation xml:lang="fr">Élément de base du schéma XML pour l'échange de données entre la liste des cours et les tiers (V 2.2.0/05.11.2025).</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="additionalPart">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für den Adresszusatz der Gesellschaft.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition du type pour le complément d'adresse de la société.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
