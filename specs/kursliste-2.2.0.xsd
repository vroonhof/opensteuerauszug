	<xs:complexType name="currencyNote">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Entität der Devisen, Banknoten und Devisen-Jahresmittelkurse.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour l'entité des devises, billets de banque et cours annuels moyens des devises.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:security">
				<xs:sequence>
					<xs:element name="yearend" type="kursliste:yearendCurrencyNote" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="de">Jahresendkurs zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Cours annuel du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="daily" type="kursliste:daily" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Tageskurse zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Cours du jour d'un titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="payment" type="kursliste:paymentCurrencyNote" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Ausschüttung zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Versement du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="legend" type="kursliste:legend" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Legende im Bezug auf den Titel bzw. auf Ereignisse zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Légende en rapport au titre resp. à l'événement du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="country" type="kursliste:countryISO2" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code des Landes zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code du pays du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="currency" type="kursliste:currencyISO3" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code der Währung zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code de la monnaie du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="denomination" type="kursliste:denomination" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Stückelung zum Jahresendkurs. Ist in aller Regel mit 1 angegeben und kann nur bei Devisen abweichen.</xs:documentation>
						<xs:documentation xml:lang="fr">Coupure du cours annuel. Sera dans tous les cas indiqué avec 1, et ne peut être différent que pour les devises.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="da1Rate">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die DA1 Sätze.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour les taux DA1.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:attribute name="country" type="kursliste:countryISO2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code des Landes.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code du pays.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="securityGroup" type="kursliste:securityGroupESTV" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Titelkategorie zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">Groupe de titre du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="securityType" type="kursliste:securityTypeESTV" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Titelart zum Titel. Falls keine Titelart angegeben ist, gilt der Wert für alle nicht aufgeführten Titelarten zur Titelkategorie.</xs:documentation>
						<xs:documentation xml:lang="fr">Type de titre du titre. Dans le cas où le type de titre n'est pas indiqué, sera en vigueur la valeur pour tous les types de titre non spécifiés du groupe de titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="da1RateType" type="kursliste:da1RateType" use="optional" default="OTHER">
					<xs:annotation>
						<xs:documentation xml:lang="de">Typ zum DA1-Satz.</xs:documentation>
						<xs:documentation xml:lang="fr">Type de taux DA1.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="validFrom" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Gültig-Ab-Datum des Prozentwerts. Dieses Datum ist nur dann angegeben, sofern der Prozentwert nach dem 1.1. des Jahres gültig ist.</xs:documentation>
						<xs:documentation xml:lang="fr">Date de validité du pourcentage à partir de. Cette date n'est indiquée que si le pourcentage est valable qu'après le 1.1. de l'année.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="validTo" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Gültig-Bis-Datum des Prozentwerts. Dieses Datum ist nur dann angegeben, sofern der Prozentwert nicht bis zum 31.12. des Jahres gültig ist.</xs:documentation>
						<xs:documentation xml:lang="fr">Date de validité du pourcentage jusqu'à. Cette date ne sera indiquée que si le pourcentage n'est pas valable jusqu'au 31.12. de l'année.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="value" type="kursliste:percent">
					<xs:annotation>
						<xs:documentation xml:lang="de">Prozentwert zum DA1-Satz. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Pourcentage du taux DA1. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="release" type="kursliste:percent">
					<xs:annotation>
						<xs:documentation xml:lang="de">Entlastung in Prozent. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Dégrèvement en pour cent. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="nonRecoverable" type="kursliste:percent">
					<xs:annotation>
						<xs:documentation xml:lang="de">Nicht rückforderbare Steuer. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Impôt non récupérable. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="nonRecoverableMax" type="kursliste:percent">
					<xs:annotation>
						<xs:documentation xml:lang="de">Höchstsatz gemäss Abkommen. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Taux maximal selon l'accord. Dans ce cas, 100 % = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="daily">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für Tageskurse zum Titel.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour le cours du marché du titre.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="remark" type="kursliste:langText" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Hinweis zum Tageskurs.</xs:documentation>
					<xs:documentation xml:lang="fr">Remarque concernant le cours du jour.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="date" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Stichtag für den Tageskurs zum Titel.</xs:documentation>
				<xs:documentation xml:lang="fr">Date du cours du marché du titre.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="currency" type="kursliste:currencyISO3" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">ISO-Code der Währung zum Titel.</xs:documentation>
				<xs:documentation xml:lang="fr">Code ISO de la monnaie de la valeur fiscale.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="quotationType" type="kursliste:quotationType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Notationsart in Stück oder Prozent.</xs:documentation>
				<xs:documentation xml:lang="fr">Type de notation en pièce ou en pour cent.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="nominalValue" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Nominalwert des Titels.</xs:documentation>
				<xs:documentation xml:lang="fr">Valeur nominale du titre.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="percent" type="kursliste:percent" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Steuerwert des Titels in Prozent. Dieser Prozentwert ist nur dann angegeben, falls der Steuerwert zum Titel in Prozent angegeben ist. Hierbei entspricht 100 = 100%.</xs:documentation>
				<xs:documentation xml:lang="fr">Valeur fiscale du titre en pour cent. Cette valeur en pour cent n'est indiquée que lorsque la valeur fiscale du titre est indiquée en pour cent.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="taxValue" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Tageskurs des Titels pro Stück.</xs:documentation>
				<xs:documentation xml:lang="fr">Cours du marché du titre à une date.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="exchangeRate" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Wechselkurs zum Stichtag.</xs:documentation>
				<xs:documentation xml:lang="fr">Cours du change à la date.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="taxValueCHF" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Tageskurs des Titels pro Stück in CHF.</xs:documentation>
				<xs:documentation xml:lang="fr">Cours du marché du titre à une date en CHF.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="undefined" type="xs:boolean" use="optional" default="0">
			<xs:annotation>
				<xs:documentation xml:lang="de">Definiert ob der Tageskurs undefiniert ist.</xs:documentation>
				<xs:documentation xml:lang="fr">Définit si le cours du marché est non défini.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="derivative">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Entität der Derivativen Finanzinstrumente.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour l'entité des instruments financiers dérivés.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:security">
				<xs:sequence>
					<xs:element name="yearend" type="kursliste:yearend" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="de">Jahresendkurs zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Cours annuel du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="daily" type="kursliste:daily" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Tageskurse zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Cours du jour d'un titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="bondfloor" type="kursliste:bondfloor" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Bondfloor Pricing zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Bondfloor Pricing du marché du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="payment" type="kursliste:paymentDerivative" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Ausschüttung zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Versement du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="legend" type="kursliste:legend" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Legende im Bezug auf den Titel bzw. auf Ereignisse zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Légende en rapport au titre resp. à l'événement du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="institutionId" type="xs:long" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Eindeutiger, technischer Schlüssel der Gesellschaft in der Kursliste.</xs:documentation>
						<xs:documentation xml:lang="fr">Clé claire, technique de la société dans la liste des cours.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="institutionName" type="kursliste:institutionName" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bezeichnung der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Désignation de la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="institutionAppendix" type="kursliste:institutionAppendix" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zusatz zur Bezeichnung der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Supplément pour la désignation de la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="country" type="kursliste:countryISO2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code des Landes zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code du pays du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="currency" type="kursliste:currencyISO3" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code der Währung zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code de la monnaie du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="issueDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Emissionsdatum zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">Date d'émission du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="redemptionDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Rückzahlungsdatum des Titels.</xs:documentation>
						<xs:documentation xml:lang="fr">Date de remboursement du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="redemptionDateEarly" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Vorzeitiges Rückzahlungsdatum des Titels.</xs:documentation>
						<xs:documentation xml:lang="fr">Remboursement anticipé d'un titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="issuePrice" type="kursliste:percent" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Emissionspreis der Bondkomponente zum Titel. Hierbei entsprechen 100% = 100. Der Emissionspreis der Bondkomponente wird vom Emittenten mitgeteilt.</xs:documentation>
						<xs:documentation xml:lang="fr">Prix d'émission de la composante obligataire du titre. Dans ce cas 100% = 100. Le prix d'émission de la composante obligataire sera transmis par l'émetteur.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="redemptionPrice" type="kursliste:percent" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Rückzahlungspreis des Titels. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Prix de remboursement du titre. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="redemptionPriceEarly" type="kursliste:percent" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Rückzahlungspreis des Titels bei vorzeitiger Rückzahlung. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Prix de remboursement anticipé du titre. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="nominalValue" type="xs:decimal" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Nominalwert zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">Valeur nominale du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="interestRate" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zinssatz zum Titel in Prozent. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Taux d'intérêt d'un titre en pour cent. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="interestRateInterest" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zinsanteil zum Titel in Prozent. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Taux d'intérêt d'un titre en pour-cent. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="interestRateOption" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Optionsprämienanteil zum Titel in Prozent. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Part de la prime d'option du titre en pour-cent. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="interestType" type="kursliste:interestType" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zinsart zum Titel. Diese kann fix oder variabel sein.</xs:documentation>
						<xs:documentation xml:lang="fr">Type d'intérêt d'un titre. Celui-ci peut être fixe ou variable.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="maturityUnlimited" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass eine unbegrenzte Laufzeit signalisiert.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique une durée indéterminée.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="pureDifferentialFrom" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Datum, ab dem der Titel der reinen Differenzbesteuerung unterliegt. Dies ist wichtig im Zusammenhang mit der BFP Berechnung, da das BFP-Flag am Titel gesetzt bleibt, wenn der Titel zuvor der modifizierten Differenzbesteuerung unterlag, nun aber nach z.B. einer Laufzeitverlängerung durch den Schuldner der reinen Differenzbesteuerung unterliegt.</xs:documentation>
						<xs:documentation xml:lang="fr">Date à partir de laquelle le titre est soumis à l'imposition de la différence. Ceci est important dans le cadre du calcul au moyen du Bond Floor Pricing (BFP), car le signal BFP se rapportant au titre reste inchangé, si le titre faisait l'objet auparavant de l'imposition de la différence selon la méthode analytique, mais que seulement après p.ex. une prolongation de la durée par un débiteur il est soumis à l'imposition de la différence.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="fund">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Entität der Anlagefonds.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour l'entité des fonds de placement.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:security">
				<xs:sequence>
					<xs:element name="yearend" type="kursliste:yearend" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="de">Jahresendkurs zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Cours annuel du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="daily" type="kursliste:daily" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Tageskurse zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Cours du jour d'un titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="payment" type="kursliste:paymentFund" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Ausschüttung zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Versement du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="legend" type="kursliste:legend" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Legende im Bezug auf den Titel bzw. auf Ereignisse zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Légende en rapport au titre resp. à l'événement du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="institutionId" type="xs:long" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Eindeutiger, technischer Schlüssel der Gesellschaft in der Kursliste.</xs:documentation>
						<xs:documentation xml:lang="fr">Clé claire, technique de la société dans la liste des cours.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="institutionName" type="kursliste:institutionName" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bezeichnung der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Désignation de la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="institutionAppendix" type="kursliste:institutionAppendix" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zusatz zur Bezeichnung der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Supplément pour la désignation de la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="country" type="kursliste:countryISO2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code des Landes zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code du pays du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="currency" type="kursliste:currencyISO3" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code der Währung zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code de la monnaie du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="nominalValue" type="xs:decimal" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Nominalwert des Titels.</xs:documentation>
						<xs:documentation xml:lang="fr">Valeur nominale du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="entity" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Entität.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour l'entité.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:attribute name="id" type="xs:long" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Eindeutiger, technischer Schlüssel der Entität.</xs:documentation>
				<xs:documentation xml:lang="fr">Clé claire, technique de l'entité.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="deleted" type="xs:boolean" use="optional" default="0">
			<xs:annotation>
				<xs:documentation xml:lang="de">Flag das signalisiert, ob die Entität gelöscht wurde.</xs:documentation>
				<xs:documentation xml:lang="fr">Signal qui indique, si l'entité sera effacée.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="exchangeRate">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für einen Devisenkurs.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour un cours des devises.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:attribute name="currency" type="kursliste:currencyISO3" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">ISO-Code der Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">ISO-code de la monnaie.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="date" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Datum zum Wechselkurs.</xs:documentation>
				<xs:documentation xml:lang="fr">Date du cours de change.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="denomination" type="kursliste:denomination" use="optional" default="1">
			<xs:annotation>
				<xs:documentation xml:lang="de">Stückelung zur Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Coupure de la monnaie.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="value" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Wechselkurs zum Datum und Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Cours de change pour une date et une monnaie.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="exchangeRateMonthly">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für einen Monatsmitteldevisenkurs.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour un cours mensuel moyen des devises.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:attribute name="currency" type="kursliste:currencyISO3" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">ISO-Code der Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">ISO-code de la monnaie.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="year" type="xs:gYear" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Jahr zum Wechselkurs.</xs:documentation>
				<xs:documentation xml:lang="fr">Année du cours de change.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="month" type="kursliste:month" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Monat zum Wechselkurs.</xs:documentation>
				<xs:documentation xml:lang="fr">Mois pour un cours de change.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="denomination" type="kursliste:denomination" use="optional" default="1">
			<xs:annotation>
				<xs:documentation xml:lang="de">Stückelung zur Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Coupure de la monnaie.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="value" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Wechselkurs zum Monat und Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Cours de change pour un mois et une monnaie.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="exchangeRateYearEnd">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für einen Jahresenddevisenkurs.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour un cours annuel des devises.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:attribute name="currency" type="kursliste:currencyISO3" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">ISO-Code der Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">ISO-code de la monnaie.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="year" type="xs:gYear" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Jahr zum Wechselkurs.</xs:documentation>
				<xs:documentation xml:lang="fr">Année du cours de change.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="denomination" type="kursliste:denomination" use="optional" default="1">
			<xs:annotation>
				<xs:documentation xml:lang="de">Stückelung zur Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Coupure de la monnaie.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="value" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Wechselkurs zum Jahr und Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Cours de change pour une année et une monnaie.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="valueMiddle" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Jahresmittelkurs zur Währung.</xs:documentation>
				<xs:documentation xml:lang="fr">Cours annuel moyen d'une monnaie.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="institution">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Entität der Gesellschaft.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour l'entité de la société.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:sequence>
					<xs:element name="remark" type="kursliste:langText" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Hinweis zur Gesellschaft.</xs:documentation>
							<xs:documentation xml:lang="fr">Indication sur la société.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="uid" type="kursliste:uid" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Unternehmens-Identifikationsnummer gemäss Bundesamt für Statistik (BFS).</xs:documentation>
						<xs:documentation xml:lang="fr">Numéro d'identification de société selon l'office fédéral de la statistique (OFS).</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="gkey" type="kursliste:gkey" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Id der Gesellschaft von SIX.</xs:documentation>
						<xs:documentation xml:lang="fr">Identifiant de la société auprès de SIX.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="institutionName" type="kursliste:institutionName" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bezeichnung der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Désignation de la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="institutionAppendix" type="kursliste:institutionAppendix" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zusatz zur Bezeichnung der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Supplément pour la désignation de la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="additionalPart" type="kursliste:additionalPart" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Adresszusatz zur Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Complément d'adresse pour la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="street" type="kursliste:street" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Strasse aus der Adresse der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Rue provenant de l'adresse de l'entreprise.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="zip" type="kursliste:zip" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Postleitzahl aus der Adresse der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Code postal provenant de l'adresse de l'entreprise.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="city" type="kursliste:city" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Ort aus der Adresse der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Lieu de l'adresse de la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="canton" type="kursliste:cantonBFS" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Abkürzung Kantonsname aus der Adresse der Gesellschaft. Die Abkürzung Kantonsname ist nur für CH-Unternehmen relevant.</xs:documentation>
						<xs:documentation xml:lang="fr">Abréviation du canton provenant de l'adresse de la société. L'abréviation du canton n'est relevante que pour les sociétés suisses.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="country" type="kursliste:countryISO2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code des Landes zur Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code du pays de la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="domain" type="kursliste:domain" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Domäne der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Domaine de la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="legalForm" type="kursliste:legalFormBUR" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Rechtsform zur Gesellschaft gemäss Handelsregister bzw. Betriebs- und Unternehmensverzeichnis (BUR).</xs:documentation>
						<xs:documentation xml:lang="fr">Forme juridique de l'entreprise selon le registre du commerce respectivement le registre des entreprises et des établissements (REE).</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="sector" type="kursliste:sectorISIC" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Branchencode zur Gesellschaft gemäss International Standard Industrial Classification (ISIC).</xs:documentation>
						<xs:documentation xml:lang="fr">Code d'activité de l'entreprise selon International Standard Industrial Classification (ISIC).</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="institutionNameOld" type="kursliste:institutionName" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Alte Bezeichnung der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Ancienne désignation de la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="langName">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Kombination aus Sprache und Name.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour la combinaison de la langue et du nom.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:attribute name="lang" type="kursliste:langISO2" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Sprache zum Namen.</xs:documentation>
				<xs:documentation xml:lang="fr">Langue du nom.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="kursliste:text" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Name in der angegebenen Sprache.</xs:documentation>
				<xs:documentation xml:lang="fr">Nom dans une langue indiquée.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="langText">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefintion für einen mehrsprachigen Text.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition d'un texte multilingue.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:attribute name="lang" type="kursliste:langISO2" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Zweistelliger ISO-Code zur Sprache.</xs:documentation>
				<xs:documentation xml:lang="fr">Code ISO à deux caractères pour la langue.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="text" type="kursliste:text" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Text zum Hinweis.</xs:documentation>
				<xs:documentation xml:lang="fr">Texte du commentaire.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="langTextMixed" mixed="true">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefintion für einen mehrsprachigen Text.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition d'un texte multilingue.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:attribute name="lang" type="kursliste:langISO2" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Zweistelliger ISO-Code zur Sprache.</xs:documentation>
				<xs:documentation xml:lang="fr">Code ISO à deux caractères pour la langue.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="legalForm">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Rechtsform gemäss Handelsregister bzw. Betriebs- und Unternehmensregister (BUR).</xs:documentation>
			<xs:documentation xml:lang="fr">Définition de la forme juridique selon le registre du commerce respectivement le registre des entreprises et des établissements (REE).</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:sequence>
					<xs:element name="legalFormName" type="kursliste:langName" minOccurs="3" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Sprache und Bezeichnung zur BUR Rechtsform.</xs:documentation>
							<xs:documentation xml:lang="fr">Langue et désignation de la forme juridique (REE).</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="legalForm" type="kursliste:legalFormBUR" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">BUR-Code zur Rechtsform.</xs:documentation>
						<xs:documentation xml:lang="fr">Code REE de la forme juridique.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="legend">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Legende zum Titel oder Ausschüttung.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour la légende du titre ou du versement.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:sequence>
					<xs:element name="text" type="kursliste:langTextMixed" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Text zum Ereignis.</xs:documentation>
							<xs:documentation xml:lang="fr">Texte de l'événement.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="eventIdSIX" type="kursliste:eventIdSIX" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Id des Ereignis oder der zusammengesetzten Id (Meldungsgegenstand, Meldungsdatum und Meldungsnummer) von SIX.</xs:documentation>
						<xs:documentation xml:lang="fr">Identifiant de l'événement ou identifiant composite (objet du message, date du message et numéro du message) de SIX.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="effectiveDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Effektivdatum zum Ereignis.</xs:documentation>
						<xs:documentation xml:lang="fr">Date effective de l'événement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="effectiveDateTo" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Effektiv-Bis-Datum zum Ereignis.</xs:documentation>
						<xs:documentation xml:lang="fr">Date déterminante jusqu'à la date de clôture de l'événement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="effectiveOrder" type="xs:positiveInteger" use="optional" default="1">
					<xs:annotation>
						<xs:documentation xml:lang="de">Reihenfolge des Ereignis, sofern mehrere Ereignisse zum gleichen Effektivdatum angegeben sind.</xs:documentation>
						<xs:documentation xml:lang="fr">Séquence des événements, pour autant que plusieurs événements soient indiqués à la même date effective.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="exchangeRatioAvailable" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass signalisiert, ob das Bezugsverhältnis zum Split/Umtausch in der Form vorhandene:neue Titel dargestellt werden kann. Die exakte Beschreibung eines Split/Umtausch wird im Text zum Ereignis dargestellt.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique si le rapport d'échange d'un split/échange dans la forme donnée: nouveau titre peut être transmis. La désignation exacte d'un split/échange sera transmis dans le texte de l'événement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="exchangeRatioPresent" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bezugsverhältnis für die vorhandenen Titel vor dem Split/Umtausch. Falls das Bezugsverhältnis vorhandene:neue Titel z.B. 5:1 ist, dann wird hier 5 dargestellt.</xs:documentation>
						<xs:documentation xml:lang="fr">Rapport d'échange pour les titres existants avant le split/échange. Au cas où le rapport est donné comme p.ex. nouveau titre 5:1, alors 5 sera mentionné .</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="exchangeRatioNew" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bezugsverhältnis für die neuen bzw. hinzugekommenen Titel bei Split/Umtausch. Falls das Bezugsverhältnis vorhandene:neue Titel z.B. 5:1 ist, dann wird hier 1 dargestellt.</xs:documentation>
						<xs:documentation xml:lang="fr">Rapport d'échange pour les nouveaux titres resp. les titres ajoutés lors d'un split/échange. Au cas où le rapport est donné comme p.ex. nouveau titre 5:1, alors 1 sera mentionné.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="valorNumberNew" type="kursliste:valorNumber" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Neue Valorennummer nach dem Split/Umtausch. Diese ist nur dann angegeben, falls die neue Valorennummer zur vorhandenen Valorennummer abweicht.</xs:documentation>
						<xs:documentation xml:lang="fr">Nouveau numéro de valeur après le split/échange. Celui-ci n'est indiqué que dans le cas où le nouveau numéro de valeur diffère du numéro de valeur donné.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="sign" type="kursliste:signESTV" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zeichen aus der Zeichenerklärung zum Ereignis.</xs:documentation>
						<xs:documentation xml:lang="fr">Signe de l'explication des signes d'un événement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="currencyOld" type="kursliste:currencyISO3" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bisheriger ISO-Code der Währung zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code de la monnaie du titre précédent.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="currencyNew" type="kursliste:currencyISO3" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Neuer ISO-Code der Währung zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">Nouvelle ISO-code de la monnaie du titre</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="nominalValueOld" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bisheriger Nominalwert des Titels.</xs:documentation>
						<xs:documentation xml:lang="fr">Valeur nominale du titre précédent.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="nominalValueNew" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Neuer Nominalwert des Titels.</xs:documentation>
						<xs:documentation xml:lang="fr">Nouvelle valeur nominale du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="liborSwap">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Entität der Libor- und Swapsätze.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition de l'entité des taux Libor et Swap.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:security">
				<xs:sequence>
					<xs:element name="daily" type="kursliste:daily" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Tageskurse zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Cours du jour d'un titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="quarterly" type="kursliste:quarterly" minOccurs="0" maxOccurs="4">
						<xs:annotation>
							<xs:documentation xml:lang="de">Quartalswerte zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Valeurs trimestrielles du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="legend" type="kursliste:legend" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Legende im Bezug auf den Titel bzw. auf Ereignisse zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Légende en rapport au titre resp. à l'événement du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="country" type="kursliste:countryISO2" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code des Landes zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code du pays du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="currency" type="kursliste:currencyISO3" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code der Währung zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code de la monnaie du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="period" type="xs:duration" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zeitspanne zum Libor- oder Swapsatz.</xs:documentation>
						<xs:documentation xml:lang="fr">Laps de temps des taux Libor ou Swap.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="mediumTermBond">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Entität der Bank-Kassen-Obligationen.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour l'entité des obligations de caisse.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:attribute name="maturityFrom" type="xs:date" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Restlaufzeit Gültig-Ab-Datum zur Bank-Kassen-Obligation.</xs:documentation>
						<xs:documentation xml:lang="fr">Durée restant à courir valable dès (date) sur des obligations de caisse.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="maturityTo" type="xs:date" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Restlaufzeit Gültig-Bis-Datum zur Bank-Kassen-Obligation.</xs:documentation>
						<xs:documentation xml:lang="fr">Durée restant à courir valable jusqu'à (date) sur des obligations de caisse.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="interestRate" type="kursliste:percent" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zinssatz der Bank-Kassen-Obligation. Der höchste Zinssatz beinhaltet auch die Angabe für höheren Zinssätze, die nicht explizit aufgeführt werden.</xs:documentation>
						<xs:documentation xml:lang="fr">Taux d'intérêt d'une obligation de caisse. Le taux d'intérêt le plus élevé contient également l'indication des taux plus élevés qui ne sont pas mentionnés explicitement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="andHigher" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag das signalisiert, ob der höchste Zinssatz auch die Angabe für höheren Zinssätze beihandelt, die nicht explizit aufgeführt werden.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique si le taux d'intérêt le plus élevé contient également l'indication des taux plus élevés qui ne sont pas mentionnés explicitement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="price" type="kursliste:percent" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Kurs der Bank-Kassen-Obligation zum Fälligkeitszeitraum und Zinssatz.</xs:documentation>
						<xs:documentation xml:lang="fr">Cours de l'obligation de caisse pour une période d'échéance et taux d'intérêt.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="payment" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für eine Ausschüttung zum Titel.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour un versement d'un titre.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:sequence>
					<xs:element name="remark" type="kursliste:langText" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Hinweis zur Ausschüttung.</xs:documentation>
							<xs:documentation xml:lang="fr">Commentaire sur versement.</xs:documentation>
							<xs:documentation xml:lang="it">Commento su reddito.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="paymentIdSIX" type="kursliste:paymentIdSIX" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Id der Ausschüttung oder der zusammengesetzten Id (Meldungsgegenstand, Meldungsdatum und Meldungsnummer) von SIX.</xs:documentation>
						<xs:documentation xml:lang="fr">Identifiant de la distribution ou identifiant composite (objet du message, date du message et numéro du message) de SIX.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="paymentDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Fälligkeitsdatum zur Ausschüttung</xs:documentation>
						<xs:documentation xml:lang="fr">Date d'échéance du versement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="currency" type="kursliste:currencyISO3" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code der Währung zur Ausschüttung.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code de la monnaie du versement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="percent" type="kursliste:percent" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Prozentwert zur Ausschüttung. Dieser Prozentwert ist nur dann angegeben, falls die Ausschüttung in Prozent angegeben ist. Hierbei entspricht 100 = 100%.</xs:documentation>
						<xs:documentation xml:lang="fr">Pourcentage du versement. Ce pourcentage n'est indiqué, que dans le cas où le versement est exprimé en pour-cent. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="paymentValue" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Betrag der Ausschüttung in der angegebenen Währung.</xs:documentation>
						<xs:documentation xml:lang="fr">Montant du versement dans la monnaie indiquée.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="exchangeRate" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Wechselkurs zur Ausschüttung.</xs:documentation>
						<xs:documentation xml:lang="fr">Cours de change d'un versement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="paymentValueCHF" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Betrag der Ausschüttung in CHF.</xs:documentation>
						<xs:documentation xml:lang="fr">Montant du versement en CHF.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="withHoldingTax" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass signalisiert, ob die Ausschüttung der Verrechnungssteuer unterliegt.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal, qui indique si le versement est soumis à l'impôt anticipé.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="undefined" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Definiert ob die Ausschüttung noch ermittelt werden muss.</xs:documentation>
						<xs:documentation xml:lang="fr">Définit si la distribution doit encore être annoncée.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="sign" type="kursliste:signESTV" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zeichen aus der Zeichenerklärung zur Ausschüttung.</xs:documentation>
						<xs:documentation xml:lang="fr">Signes de l'explication des signes d'un versement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="paymentType" type="kursliste:paymentTypeESTV" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Art des steuerlichen Ereignis.</xs:documentation>
						<xs:documentation xml:lang="fr">Type de l'événement imposable.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="taxEvent" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass signalisiert, ob die Ausschüttung lediglich ein steuerlich relevantes Ereignis darstellt.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique si le paiement représente un événement fiscalement pertinent.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="variant" type="kursliste:variant" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Die Variante im Bezug zum Titel und Ex-Datum zum Ereignis.Ist eine Variante angegeben, so muss eine Wahl (ODER) zwischen einer der Varianten für die Erträge getroffen werden. Die unterschiedlichen Varianten zum Ereignis können hierbei über den Titgel bzw. das Fälligkeitsdatum ermittelt werden.Mehrere Erträge zur Variante werden durch die gleiche Variante miteinander (UND) verknüpft. Es kann nicht davon ausgegangen werden, dass die gleiche Nummer einer Variante sich immer auf eine Barausschüttung oder Stockausschüttung bezieht.</xs:documentation>
						<xs:documentation xml:lang="fr">Les variantes de l'événement. Si une variante est indiquée, un choix (OU) entre une des variantes pour les rendements doit être fait. Les différentes variantes d'un événement peuvent être déterminées  dans ce cas sur le titre respectivement la date d'échéance. Plusieurs rendements d'une variante sont liés ensemble par la même variante (ET).</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="paymentBond">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für eine Ausschüttung zu Obligationen.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour un versement d'une obligation.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:payment">
				<xs:sequence>
					<xs:element name="legend" type="kursliste:legend" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Legende im Bezug auf die Ausschüttung.</xs:documentation>
							<xs:documentation xml:lang="fr">Légende en rapport au versement.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="issueDisagio" type="kursliste:percent" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Emissionsdisagio in Prozent.</xs:documentation>
						<xs:documentation xml:lang="fr">Disagio d'émission en pour cent.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="redemptionAgio" type="kursliste:percent" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Rückzahlungsagio in Prozent.</xs:documentation>
						<xs:documentation xml:lang="fr">Agio de remboursement en pour cent.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="paymentCurrencyNote">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für eine Ausschüttung zu Devisen.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour un versement d'une en devises.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:payment">
				<xs:sequence>
					<xs:element name="legend" type="kursliste:legend" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Legende im Bezug auf die Ausschüttung.</xs:documentation>
							<xs:documentation xml:lang="fr">Légende en rapport au versement.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="paymentDerivative">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für eine Ausschüttung zu Derivativen Finanzinstrumenten.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour un versement d'un produit financier dérivé.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:payment">
				<xs:sequence>
					<xs:element name="legend" type="kursliste:legend" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Legende im Bezug auf die Ausschüttung.</xs:documentation>
							<xs:documentation xml:lang="fr">Légende en rapport au versement.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="exDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Ex Datum zur Ausschüttung.</xs:documentation>
						<xs:documentation xml:lang="fr">Date ex du versement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="coupon" type="kursliste:coupon" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Coupon zur Ausschüttung.</xs:documentation>
						<xs:documentation xml:lang="fr">Coupon du versement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="paymentFund">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für eine Ausschüttung zu Anlagefonds.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour un versement d'un fonds de placement.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:payment">
				<xs:sequence>
					<xs:element name="legend" type="kursliste:legend" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Legende im Bezug auf die Ausschüttung.</xs:documentation>
							<xs:documentation xml:lang="fr">Légende en rapport au versement.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="exDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Ex Datum zur Ausschüttung.</xs:documentation>
						<xs:documentation xml:lang="fr">Date ex du versement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="coupon" type="kursliste:coupon" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Coupon zur Ausschüttung.</xs:documentation>
						<xs:documentation xml:lang="fr">Coupon du versement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="capitalGain" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass signalisiert, ob die Ausschüttung einen steuerfreien Kapitalgewinn darstellt.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique si le versement représente un gain en capital non imposable.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="incomeType" type="kursliste:incomeType" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Ertragsart der Ausschüttung.</xs:documentation>
						<xs:documentation xml:lang="fr">Type de paiement de revenu.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="deduction" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Abzug der zurück zu erstattenden Verrechnungssteuer in Fremdwährung.</xs:documentation>
						<xs:documentation xml:lang="fr">Déduction de la retenue d'impôt au remboursement en devises.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="deductionCHF" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Abzug der zurück zu erstattenden Verrechnungssteuer in CHF.</xs:documentation>
						<xs:documentation xml:lang="fr">Déduction de la retenue d'impôt au remboursement en CHF.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="deductionDividend" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Abzug der zurück zu erstattenden Verrechnungssteuer auf Dividenden in Fremdwährung.</xs:documentation>
						<xs:documentation xml:lang="fr">Déduction de la retenue d'impôt au remboursement les dividendes en devises.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="deductionDividendCHF" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Abzug der zurück zu erstattenden Verrechnungssteuer auf Dividenden in CHF.</xs:documentation>
						<xs:documentation xml:lang="fr">Déduction de la retenue d'impôt au remboursement les dividendes en CHF.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="deductionInterest" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Abzug der zurück zu erstattenden Verrechnungssteuer auf Zinsen in Fremdwährung.</xs:documentation>
						<xs:documentation xml:lang="fr">Déduction de la retenue d'impôt à rembourser les intérêts en devises.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="deductionInterestCHF" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Abzug der zurück zu erstattenden Verrechnungssteuer auf Zinsen in CHF.</xs:documentation>
						<xs:documentation xml:lang="fr">Déduction de la retenue d'impôt à rembourser les intérêts en CHF.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="paymentShare">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für eine Ausschüttung zu Aktien, Stammanteile und Anteilscheine.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour un versement d'une action.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:payment">
				<xs:sequence>
					<xs:element name="legend" type="kursliste:legend" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Legende im Bezug auf die Ausschüttung.</xs:documentation>
							<xs:documentation xml:lang="fr">Légende en rapport au versement.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="exDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Ex Datum zur Ausschüttung.</xs:documentation>
						<xs:documentation xml:lang="fr">Date ex du versement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="coupon" type="kursliste:coupon" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Coupon zur Ausschüttung.</xs:documentation>
						<xs:documentation xml:lang="fr">Coupon du versement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="gratis" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass eine Gratisaktien-Auschüttung signalisiert.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique une distribution d'actions gratuites.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="portefeuille" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass eine Portefeuille-Auschüttung signalisiert.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique une distribution de titres.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="quarterly">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Quartalswerte zum Titel.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition des valeurs trimestrielles du titre.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:daily">
				<xs:attribute name="quarter" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Quartal zum Quartalswert.</xs:documentation>
						<xs:documentation xml:lang="fr">Trimestre de la valeur trimestrielle.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:integer">
							<xs:minInclusive value="1"/>
							<xs:maxInclusive value="4"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="security" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Entität des Titels.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour l'entité des titres.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:sequence>
					<xs:element name="remark" type="kursliste:langText" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Hinweis zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Indication sur le titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="valorNumber" type="kursliste:valorNumber" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Eindeutiger Schlüssel des Titels.</xs:documentation>
						<xs:documentation xml:lang="fr">Clé claire du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="isin" type="kursliste:isin" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Internationale Securities Identifikationsnummer des Titels.</xs:documentation>
						<xs:documentation xml:lang="fr">Numéro d'identification international de sécurité (ISIN) du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="securityGroup" type="kursliste:securityGroupESTV" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Titelkategorie zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">Groupe de titre du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="securityType" type="kursliste:securityTypeESTV" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Titelart zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">Type de titre du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="securityName" type="kursliste:securityName" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bezeichnung des Titels ohne redundante Informationen wie Zinssatz, Emissions- und Rückzahlungsdatum.</xs:documentation>
						<xs:documentation xml:lang="fr">Désignation du titre sans informations redondantes tel que taux d'intérêt, date d'émission et de remboursement.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="securityAppendix" type="kursliste:securityAppendix" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zusätzliche Bezeichnung zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">Désignation supplémentaire du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="sign" type="kursliste:signESTV" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zeichen aus der Zeichenerklärung zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">Signes de l'explication des signes d'un titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="iup" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass die überwiegend einmalige Verzinsung des Titels signalisiert.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique l'intérêt unique prédominant d'un titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="bfp" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass die Berücksichtigung des Titels im Bond Floor Pricing signalisiert.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique la prise en considération du titre dans le Bond Floor Pricing.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="validity" type="kursliste:validity" use="optional" default="DEFINITIVE">
					<xs:annotation>
						<xs:documentation xml:lang="de">Gültigkeit einer Lieferung.</xs:documentation>
						<xs:documentation xml:lang="fr">Validité d'une livraison.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="quoted" type="xs:boolean" use="optional" default="1">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass die Kotiertung des Titels signalisiert.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique la cotation du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="indefaultDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Notleidend-Datum des Titels.</xs:documentation>
						<xs:documentation xml:lang="fr">Date par défaut du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="liquidationDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Liquidationsdatum des Titels.</xs:documentation>
						<xs:documentation xml:lang="fr">Date de liquidation du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="inactiveDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Inaktivdatum des Titels.</xs:documentation>
						<xs:documentation xml:lang="fr">Date inactif du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="securityGroup">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die mehrsprachige Beschreibung der Titelkategorien.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour la description en plusieurs langues du groupe du titre.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:sequence>
					<xs:element name="securityGroupName" type="kursliste:langName" minOccurs="3" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Sprache und Bezeichnung zur Titelkategorie.</xs:documentation>
							<xs:documentation xml:lang="fr">Langue et désignation du groupe de titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="securityGroup" type="kursliste:securityGroupESTV" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Titelkategorie zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">Groupe de titre du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="securityType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die mehrsprachige Beschreibung der Titelart.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour la description en plusieurs langues du type du titre.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:sequence>
					<xs:element name="securityTypeName" type="kursliste:langName" minOccurs="3" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Sprache und Bezeichnung zur Titelart.</xs:documentation>
							<xs:documentation xml:lang="fr">Langue et désignation du type de titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="securityType" type="kursliste:securityTypeESTV" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Titelart zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">Type de titre du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="scope" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für einen Gültigkeitsbereich.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour une période de validité.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:attribute name="validFrom" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Gültig-Ab-Datum des Gültigkeitsbereichs.</xs:documentation>
				<xs:documentation xml:lang="fr">Date de la période de validité à partir de.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="validTo" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Gültig-Bis-Datum des Gültigkeitsbereichs. Eine unbegrenzte Gültigkeit wird hierbei durch den 31.12.9999 signalisiert.</xs:documentation>
				<xs:documentation xml:lang="fr">Date de la période de validité jusqu'à. Une validité indéterminée sera signalisée par le 31.12.9999.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="sector">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für den Branchencode gemäss International Standard Industrial Classification (ISIC).</xs:documentation>
			<xs:documentation xml:lang="fr">Définition du code d'activité selon International Standard Industrial Classification (ISIC).</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:sequence>
					<xs:element name="sectorName" type="kursliste:langName" minOccurs="3" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Sprache und Bezeichnung zum Branchencode.</xs:documentation>
							<xs:documentation xml:lang="fr">Langue et désignation du code d'activité.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="sector" type="kursliste:sectorISIC" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISIC-Code zur Branche.</xs:documentation>
						<xs:documentation xml:lang="fr">Code ISIC du code d'activité.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="share">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Entität der Aktie.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour l'entité des actions.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:security">
				<xs:sequence>
					<xs:element name="yearend" type="kursliste:yearend" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="de">Jahresendkurs zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Cours annuel du titre</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="daily" type="kursliste:daily" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Tageskurse zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Cours du jour d'un titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="payment" type="kursliste:paymentShare" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Ausschüttung zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Versement du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
					<xs:element name="legend" type="kursliste:legend" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Legende im Bezug auf den Titel bzw. auf Ereignisse zum Titel.</xs:documentation>
							<xs:documentation xml:lang="fr">Légende en rapport au titre resp. à l'événement du titre.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="institutionId" type="xs:long" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Eindeutiger, technischer Schlüssel der Gesellschaft in der Kursliste.</xs:documentation>
						<xs:documentation xml:lang="fr">Clé claire, technique de la société dans la liste des cours.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="institutionName" type="kursliste:institutionName" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bezeichnung der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Désignation de la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="institutionAppendix" type="kursliste:institutionAppendix" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zusatz zur Bezeichnung der Gesellschaft.</xs:documentation>
						<xs:documentation xml:lang="fr">Supplément pour la désignation de la société.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="country" type="kursliste:countryISO2" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code des Landes zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code du pays du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="currency" type="kursliste:currencyISO3" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ISO-Code der Währung zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">ISO-code de la monnaie du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="nominalValue" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Nominalwert des Titels.</xs:documentation>
						<xs:documentation xml:lang="fr">Valeur nominale du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="liberalized" type="kursliste:percent" use="optional" default="100">
					<xs:annotation>
						<xs:documentation xml:lang="de">Liberierungsgrad zum Titel. Diese ist nur angegeben wenn diese von 100% abweicht. Hierbei entsprechen 100 = 100%.</xs:documentation>
						<xs:documentation xml:lang="fr">Taux de libération du titre. Celui-ci n'est indiqué que s'il diffère de 100%. Dans ce cas 100 = 100%.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="gratis" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass die Aufnahme in die Kursliste aufgrund einer Gratisaktie signalisiert.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique la saisie dans la liste des cours sur la base d'une action gratuite.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="capitalKey" type="kursliste:capitalKeyESTV" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Kapitalschlüssel zum Titel (nur für CH-Titel relevant).</xs:documentation>
						<xs:documentation xml:lang="fr">Clé de capital du titre (seulement relevant pour les titres suisses).</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="valorNumberUnderlying" type="kursliste:valorNumber" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Valorennummer des Titels, von dem der Titel (ADR, GDR,...) abhängig ist z.B. VN: 72282 (GDR) zu VN: 722288.</xs:documentation>
						<xs:documentation xml:lang="fr">Numéro de valeur du titre, dont le titre (ADR, GDR,...) est dépendant, p.ex. NV: 72282 (GDR) pour NV: 722288.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ratioUnderlying" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Verhältnis zwischen dem Titel (ADR, GDR,...) und dem Titel, von dem dieser Titel (ADR, GDR,...) abhängig ist z.B. 1x VN: 722282 (GDR) = 0.5x VN: 722288 = 0.5.</xs:documentation>
						<xs:documentation xml:lang="fr">Rapport entre le titre (ADR, GDR,...) et le titre, dont ce titre  (ADR, GDR,...) est dépendant, p.ex. 1x NV: 72282 (GDR) = 0.5x NV: 722288 = 0.5.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="quotedDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Datum zu dem der Titel kotiert wurde. Dies ist nur angegeben, wenn es sich um einen CH-Titel handelt, der im aktuellen Jahr kotiert wurde.</xs:documentation>
						<xs:documentation xml:lang="fr">Date à partir de laquelle le titre a été coté. Ceci n'est indiqué, que lorsqu'il s'agit d'un titre suisse, qui a été coté dans le courant de l'année actuelle. </xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="nonQuotedDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Datum zu dem der Titel dekotiert wurde. Dies ist nur angegeben, wenn es sich um einen CH-Titel handelt, der im aktuellen Jahr dekotiert wurde.</xs:documentation>
						<xs:documentation xml:lang="fr">Date à partir de laquelle le titre a été décoté. Ceci n'est indiqué, que lorsqu'il s'agit d'un titre suisse, qui a été décoté dans le courant de l'année actuelle. </xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="shortCut">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für eine Erklärung zu einer Abkürzung.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour une explication d'une abréviation.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:sequence>
					<xs:element name="shortCutName" type="kursliste:langName" minOccurs="3" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Sprache und Bezeichnung zur Abkürzung.</xs:documentation>
							<xs:documentation xml:lang="fr">Langue et désignation de l'abréviation.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="shortCut" type="kursliste:shortCutESTV" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zeichen zur Abkürzung.</xs:documentation>
						<xs:documentation xml:lang="fr">Signes d'une abréviation.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="sign">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für eine Zeichenerklärung.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour une explication des signes.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:sequence>
					<xs:element name="signName" type="kursliste:langName" minOccurs="3" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Sprache und Bezeichnung zur Zeichenerklärung.</xs:documentation>
							<xs:documentation xml:lang="fr">Langue et désignation de l'explication du signe.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="sign" type="kursliste:signESTV" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zeichen zur Zeichenerklärung.</xs:documentation>
						<xs:documentation xml:lang="fr">Signes d'une explication des signes.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="yearend">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für den Jahresendkurs zum Titel.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour le cours annuel du titre.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:entity">
				<xs:sequence>
					<xs:element name="remark" type="kursliste:langText" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="de">Hinweis zum Jahresendkurs.</xs:documentation>
							<xs:documentation xml:lang="fr">Commentaire sur cours de fin d'année.</xs:documentation>
							<xs:documentation xml:lang="it">Commento su corso di fine del anno.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="quotationType" type="kursliste:quotationType" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Notationsart des Jahresendkurses in Stück oder Prozent.</xs:documentation>
						<xs:documentation xml:lang="fr">Type de notation du cours annuel en pièce ou en pour cent.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="percent" type="kursliste:percent" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Steuerwert des Titels in Prozent. Dieser Prozentwert ist nur dann angegeben, falls der Steuerwert zum Titel in Prozent angegeben ist. Hierbei entspricht 100 = 100%.</xs:documentation>
						<xs:documentation xml:lang="fr">Valeur fiscale du titre en pour cent. Cette valeur en pour cent n'est indiquée que lorsque la valeur fiscale du titre est indiquée en pour cent.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="taxValue" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Steuerwert des Titels pro Stück in der angegebenen Währung.</xs:documentation>
						<xs:documentation xml:lang="fr">Valeur fiscale du titre par pièce dans la monnaie donnée.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="exchangeRate" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Wechselkurs zum Steuerwert des Titels.</xs:documentation>
						<xs:documentation xml:lang="fr">Cours de change pour la valeur fiscale du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="taxValueCHF" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Steuerwert des Titels pro Stück in CHF.</xs:documentation>
						<xs:documentation xml:lang="fr">Valeur fiscale du titre par pièce en CHF.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="undefined" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Definiert ob der Jahresendkurs mit dem Wert 0 undefiniert ist.</xs:documentation>
						<xs:documentation xml:lang="fr">Définit si le cours final avec la valeur 0 est non défini.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="yearendCurrencyNote">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für den Jahresendkurs zur Währung zum Titel.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition du cours annuel de la monnaie du titre.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="kursliste:yearend">
				<xs:attribute name="taxValueCHFNote" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Jahresendkurs der Banknote in CHF.</xs:documentation>
						<xs:documentation xml:lang="fr">Cours annuel du billet de banque en CHF.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="taxValueCHFMiddle" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Jahresmittelkurs der Devise in CHF.</xs:documentation>
						<xs:documentation xml:lang="fr">Cours annuel moyen de la devise en CHF.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="kursliste">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefintion für das Basiselement des XML Schema zum Datenaustausch zwischen Kursliste und Dritten.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour l'élément de base du schéma XML pour l'échange de données entre la liste des cours et les tiers.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="canton" type="kursliste:canton" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu den Abkürzungen Kantonsnamen gemäss Bundesamt für Statistik (BFS).</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les abréviations du nom de canton selon l'Office fédéral de la statistique (OFS).</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="capitalKey" type="kursliste:capitalKey" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu den Kapitalschlüsseln gemäss ESTV.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les clés de capital selon l'AFC.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="country" type="kursliste:country" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zur Definition der Länder gemäss ISO (2-stellig.)</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour la définition des pays.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" type="kursliste:currency" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zur Definition der Währungen gemäss ISO (3-stellig).</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour la définition des monnaies.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="securityGroup" type="kursliste:securityGroup" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zur Erklärung der Titelkategorien.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour l'explication du groupe de titre.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="securityType" type="kursliste:securityType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zur Erklärung der Titelart.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour l'explication du type de titre.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="legalForm" type="kursliste:legalForm" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu den Rechtsformen gemäss Handelsregister bzw. Betriebs- und Unternehmensregister (BUR).</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les formes juridiques selon le registre du commerce respectivement le registre des entreprises et des établissements (REE).</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="sector" type="kursliste:sector" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu den Branchencodes gemäss International Standard Industrial Classification (ISIC).</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les codes d'activités selon International Standard Industrial Classification (ISIC).</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="shortCut" type="kursliste:shortCut" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zur Erklärung der Abkürzungen.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour l'explication des abréviations.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="sign" type="kursliste:sign" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zur Zeichenerklärung.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour l'explication des signes.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="capitalisationRate" type="kursliste:capitalisationRate" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu den Kapitalisierungssätzen.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les taux de capitalisation.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="da1Rate" type="kursliste:da1Rate" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu den DA1-Sätzen.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les taux DA1.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="mediumTermBond" type="kursliste:mediumTermBond" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität der Bank-Kassen-Obligationen.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité des obligations de caisse.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="institution" type="kursliste:institution" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität der Gesellschaft.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité de la société.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="bond" type="kursliste:bond" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu Obligationen.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les obligations.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="coinBullion" type="kursliste:coinBullion" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu Münzen und Edelmetallen.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les monnaies d'or et métaux précieux.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="currencyNote" type="kursliste:currencyNote" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu Devisen, Banknoten und Devisen-Jahresmittelkursen.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les devises, billets de banque et cours annuels moyens des devises.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="derivative" type="kursliste:derivative" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu Derivativen Finanzinstrumenten.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les produits financiers dérivés.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="fund" type="kursliste:fund" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu Anlagefonds.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les fonds de placement.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="liborSwap" type="kursliste:liborSwap" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu Libor- und Swapsätzen.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité des taux Libor et Swap.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="share" type="kursliste:share" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu Aktien inkl. Genussscheine usw.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour actions inclus les bons de jouissance, etc.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="exchangeRate" type="kursliste:exchangeRate" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu Devisenkursen.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les cours des devises.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="exchangeRateMonthly" type="kursliste:exchangeRateMonthly" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu Monatsmitteldevisenkursen.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les cours mensuels moyens des devises.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
			<xs:element name="exchangeRateYearEnd" type="kursliste:exchangeRateYearEnd" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Entität zu Jahresenddevisenkursen.</xs:documentation>
					<xs:documentation xml:lang="fr">Entité pour les cours annuels des devises.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="version" type="kursliste:version" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Version des zugehörigen Schema.</xs:documentation>
				<xs:documentation xml:lang="fr">Version du schéma correspondant.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="creationDate" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Erstelldatum der Datei</xs:documentation>
				<xs:documentation xml:lang="fr">Date d'établissement du fichier.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="referingToDate" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Bezugsdatum der Datenlieferung. Nur bei Deltalieferungen angegeben.</xs:documentation>
				<xs:documentation xml:lang="fr">Date de la livraison des données. Seulement indiquée lors d'une livraison delta.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="year" type="xs:gYear" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Jahr, auf das sich die Kursliste bezieht.</xs:documentation>
				<xs:documentation xml:lang="fr">Année à laquelle se réfère la liste des cours.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
</xs:schema>
