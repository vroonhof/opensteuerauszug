							<xs:documentation xml:lang="fr">Langue et désignation de la clé du capital.</xs:documentation>
							<xs:documentation xml:lang="it"/>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="capitalKey" type="kursliste:capitalKeyESTV" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">ESTV Kapitalschlüssel zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">Clé de capital du titre selon AFC.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
