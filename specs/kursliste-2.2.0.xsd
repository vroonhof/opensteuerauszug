				<xs:attribute name="issuePrice" type="kursliste:percent" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Emissionspreis des Titels. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Prix d'émission du titre. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="redemptionPrice" type="kursliste:percent" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Rückzahlungspreis des Titels. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Prix de remboursement du titre. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="redemptionPriceEarly" type="kursliste:percent" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Rückzahlungspreis des Titels bei vorzeitiger Rückzahlung. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Prix de remboursement anticipé du titre. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="nominalValue" type="xs:decimal" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="de">Nominalwert zum Titel.</xs:documentation>
						<xs:documentation xml:lang="fr">Valeur nominale du titre.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="interestRate" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zinssatz zum Titel in Prozent. Hierbei entsprechen 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="fr">Taux d'intérêt d'un titre en pour cent. Dans ce cas 100% = 100.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="interestType" type="kursliste:interestType" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Zinsart zum Titel. Diese kann fix oder variabel sein.</xs:documentation>
						<xs:documentation xml:lang="fr">Type d'intérêt d'un titre. Celui-ci peut être fixe ou variable.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="classicalBond" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass einen klassische Anleihe signalisiert.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique un emprunt classique.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="maturityUnlimited" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass eine unbegrenzte Laufzeit signalisiert.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique une durée indéterminée.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="liberalized" type="kursliste:percent" use="optional" default="100">
					<xs:annotation>
						<xs:documentation xml:lang="de">Liberierungsgrad zum Titel. Diese ist nur angegeben wenn diese von 100% abweicht. Hierbei entsprechen 100 = 100%.</xs:documentation>
						<xs:documentation xml:lang="fr">Taux de libération du titre. Celui-ci n'est indiqué que s'il diffère de 100%. Dans ce cas 100 = 100%.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="accruedInterest" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass die Zahlung von Bruchzinsen bei der Wandlung einer Wandelanleihe vor einem Coupontermin signalisiert.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qui indique le paiement d'un intérêt couru lors d'une conversion d'un emprunt convertible avant l'échéance du coupon.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="issuePriceAverage" type="kursliste:percent" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Durchschnittlicher Emissionspreis zum Titel, der sich aufgrund von Aufstockungen ergibt.</xs:documentation>
						<xs:documentation xml:lang="fr">Prix d'émission moyen du titre, qui ressort des augmentations.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="redemptionDateNoConversion" type="xs:boolean" use="optional" default="0">
					<xs:annotation>
						<xs:documentation xml:lang="de">Flag, dass keine Wandlung (Cleanup-Call) zum Zeitpunkt der (vorzeitigen) Rückzahlung vorsieht.</xs:documentation>
						<xs:documentation xml:lang="fr">Signal qu'aucun échange (Cleanup-Call) n'est prévu au moment du remboursement (anticipé).</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="pureDifferentialFrom" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="de">Datum, ab dem der Titel der reinen Differenzbesteuerung unterliegt. Dies ist wichtig im Zusammenhang mit der BFP Berechnung, da das BFP-Flag am Titel gesetzt bleibt, wenn der Titel zuvor der modifizierten Differenzbesteuerung unterlag, nun aber nach z.B. einer Laufzeitverlängerung durch den Schuldner der reinen Differenzbesteuerung unterliegt.</xs:documentation>
						<xs:documentation xml:lang="fr">Date à partir de laquelle le titre est soumis à l'imposition de la différence. Ceci est important dans le cadre du calcul au moyen du Bond Floor Pricing (BFP), car le signal BFP se rapportant au titre reste inchangé, si le titre faisait l'objet auparavant de l'imposition de la différence selon la méthode analytique, mais que seulement après p.ex. une prolongation de la durée par un débiteur il est soumis à l'imposition de la différence.</xs:documentation>
						<xs:documentation xml:lang="it"/>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="bondfloor">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für das Bondfloor Pricing zum Titel.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition pour le Bondfloor Pricing du titre.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="remark" type="kursliste:langText" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Hinweis zum Bondfloor Pricing.</xs:documentation>
					<xs:documentation xml:lang="fr">Remarque concernant le Bondfloor Pricing.</xs:documentation>
					<xs:documentation xml:lang="it"/>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="date" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">Stichtag für den Bondfloor zum Titel.</xs:documentation>
				<xs:documentation xml:lang="fr">Date du Bondfloor du marché du titre.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="currency" type="kursliste:currencyISO3" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="de">ISO-Code der Währung zum Titel.</xs:documentation>
				<xs:documentation xml:lang="fr">Code ISO de la monnaie de la valeur fiscale.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="quotationType" type="kursliste:quotationType" use="optional" default="PERCENT">
			<xs:annotation>
				<xs:documentation xml:lang="de">Notationsart in Stück oder Prozent.</xs:documentation>
				<xs:documentation xml:lang="fr">Type de notation en pièce ou en pour cent.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="nominalValue" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Nominalwert des Titels.</xs:documentation>
				<xs:documentation xml:lang="fr">Valeur nominale du titre.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="percent" type="kursliste:percent" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Bondfloor des Titels in Prozent. Hierbei entspricht 100 = 100%.</xs:documentation>
				<xs:documentation xml:lang="fr">Bondfloor du marché du titre en pour cent.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="taxValue" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Bondfloor des Titels pro Stück.</xs:documentation>
				<xs:documentation xml:lang="fr">Bondfloor du marché du titre à une date.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="exchangeRate" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Der Wechselkurs zum Stichtag.</xs:documentation>
				<xs:documentation xml:lang="fr">Le cours du change à la date.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="taxValueCHF" type="xs:decimal" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="de">Bondfloor des Titels pro Stück in CHF.</xs:documentation>
				<xs:documentation xml:lang="fr">Bondfloor du marché du titre à une date en CHF.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="undefined" type="xs:boolean" use="optional" default="0">
			<xs:annotation>
				<xs:documentation xml:lang="de">Definiert ob der Bondfloor undefiniert ist.</xs:documentation>
				<xs:documentation xml:lang="fr">Définit si le Bondfloor du marché est non défini.</xs:documentation>
				<xs:documentation xml:lang="it"/>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="bondIncrease">
		<xs:annotation>
			<xs:documentation xml:lang="de">Typdefinition für die Aufstockung der Obligationen.</xs:documentation>
			<xs:documentation xml:lang="fr">Définition de l'augmentation des obligations.</xs:documentation>
			<xs:documentation xml:lang="it"/>
		</xs:annotation>
